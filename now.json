{
  "version": 2,
  "name": "documentslibrary-app",
  "functions": {
    "api/webapp.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {"src": "^/(.+).(js|json|webmanifest|css)$", "dest": "public/$1.$2"},
    {"src": "^/img/(.+)$", "dest": "/public/img/$1"},
    {"src": ".*", "dest": "/api/webapp.js"}
  ]
}
