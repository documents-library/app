{
  "version": 2,
  "name": "documentslibrary-app",
  "builds": [
    {
      "src": "./public/**",
      "use": "@now/static"
    }
  ],
  "routes": [
    {"src": "^/(.+).(js|json|webmanifest|css)$", "dest": "public/$1.$2"},
    {"src": "^/img/(.+)$", "dest": "public/img/$1"},
    {"src": ".*", "dest": "public/index.html"}
  ],
  "build": {
    "env": {
      "API_URL": "@api_url"
    }
  }
}
