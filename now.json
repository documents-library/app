{
  "version": 2,
  "functions": {
    "api/webapp.js": {
      "maxDuration": 10
    }
  },
  "routes": [
    {"src": "^/(.+).(js|json|webmanifest|css)$", "dest": "/$1.$2"},
    {"src": "^/img/(.+)$", "dest": "/img/$1"},
    {"src": "/robots.txt", "dest": "/robots.txt"},
    {"src": ".*", "dest": "/index.html"}
  ]
}
